    "jobs": {
        "generic_appearance_descriptions": [
            {
                "index": "regal_golds",
                "text": "[[Name]] is dressed in regal whites and golds. [[Their]] fingers are adorned with rings socketed with exotic gems."
            },
            {
                "index": "dirty_grime",
                "text": "[[Name]]'s face is crusted with dirt and grime. [[Their]] heavy canvas clothes are worn and beginning to fray."
            },
            {
                "index": "evergreen_shirt",
                "text": "[[Name]] is clad in a lightweight leather vest and a faded evergreen shirt. Many pocket-belts hang across [[their]] torso and around [[their]] waist."
            },
            {
                "index": "red_bandana",
                "text": "[[Name]] wears a deep red bandana across their nose and mouth. [[Their]] clothes are worn and darkened, but clean.",
                "obscured": [
                    "nose",
                    "mouth"
                ]
            },
            {
                "index": "regal_furs",
                "text": "[[Name]] wears of a coat of exotic furs and leathers. The many coin-purses strapped to it jingle as [[they]] walk."
            },
            {
                "index": "shirtless_axe",
                "text": "[[Name]] carries [[themself]] shirtless, save for a few scraps of cloth on [[their]] chest. [[They]] wear only leather pants and a steel greataxe strapped across [[their]] torso"
            },
            {
                "index": "frog_friend",
                "text": "[[Name]] wears a coat of deep purple, accompanied by a live frog perched upon [[their]] shoulder. The frog croaks loudly, but seems not to otherwise move"
            },
            {
                "index": "elite_archer",
                "text": "[[Name]] wears a fine coat of dark leather, adorned with shining pins and trinkets. At [[their]] side [[they]] carry a lustrous bow, rich with ornate carvings."
            },
            {
                "index": "blue_robes",
                "text": "[[Name]] wears a long, flowing robe of rich blues and golds. Around [[their]] neck hangs many amulets and necklaces socketed with faintly glowing gems."
            },
            {
                "index": "tavern_musician",
                "text": "[[Name]] wears simple leather clothes and a small, torn hat. [[They]] carry with them a lute carved from some exotic wood and adorned with ornately-carved metal."
            },
            {
                "index": "stained_apron",
                "text": "[[Name]] wears simple clothes, covered by a large white apron stained with unknown colors. "
            }
        ],
